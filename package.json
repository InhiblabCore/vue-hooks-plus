		"@types/react-dom": "^18.0.6",
		"@typescript-eslint/eslint-plugin": "^5.33.0",
		"@typescript-eslint/parser": "^5.33.0",
		"@vitest/ui": "^0.25.3",
		"@types/fs-extra": "^9.0.13",
		"@types/jest": "^29.0.0",
		"@types/lodash": "^4.14.178",
		"@types/marked": "^4.0.3",
		"@types/node": "^17.0.21",
		"@types/qs": "^6.9.7",
		"@vitejs/plugin-vue": "^2.3.1",
    "@vue-hooks-plus/vite-plugin-gen-temp": "2.6.2",
		"@vue-hooks-plus/md-demo-plugins": "^1.0.0",
		"@vue-hooks-plus/vitepress": "1.2.3",
		"@vue-hooks-plus/vitepress-demo-block": "^1.3.1",
    "@vue-hooks-plus/use-immer":"workspace:^1.0.0",
		"@vue/test-utils": "^2.1.0",
    "@vitest/coverage-c8":"^0.25.7",
		"eslint": "^8.20.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-vue": "^9.6.0",
		"husky": "^8.0.1",
		"lint-staged": "^13.0.3",
		"vue-eslint-parser": "^9.0.3",
		"cross-env": "^7.0.3",
		"fast-glob": "^3.2.12",
		"fs-extra": "^10.1.0",
		"gh-pages": "^4.0.0",
		"happy-dom": "^7.5.10",
		"less": "^4.1.3",
		"prettier": "^1.19.1",
		"rimraf": "^3.0.2",
		"ts-morph": "^13.0.2",
		"ts-node": "^10.7.0",
		"typeit": "^8.7.0",
		"typescript": "^4.9.3",
		"vite": "3.0.2",
		"vite-plugin-build": "0.6.0",
		"vite-plugin-dts": "^1.4.1",
		"vitepress": "1.0.0-alpha.4",
		"vitest": "^0.25.3",
		"vue": "^3.2.25",
		"vue-tsc": "1.0.9",
		"vue-typical": "^2.1.0",
    "three":"0.147.0"
	},
	"lint-staged": {
		"*.{js,ts,tsx,vue}": [
			"eslint --fix"
